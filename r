public class rev
{
    String name[]= {"s", "r", "i", "n" , "u"};
    int i ;
    String a[] ;
    int Print()
           { 
             for (i = 4; i>=0; i--) 
             {
                 name[i] = a[] ; 
                      break ;
             }
             return(a[]) ;
           }
  public static void main(String[] args)
  {
    int j ;
    rev m = new rev() ;
    for (j = 4; j>=0; j--) 
    System.out.print(" "+m.Print());
  }
}
